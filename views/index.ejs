<%- include('./parts/head.ejs') %> <%# Navbar %> <%-
include('./parts/navbar.ejs') %>

<div class="container-fluid">
  <div class="row my-4">
    <div class="col-lg-12">
      <% if (message) { %>
       <p class="alert alert-dismissible fade show alert-<%= message.type %>" role="alert">
      <button class='btn-close' type="button" data-bs-dismiss="alert" aria-label="Close"></button>
      <strong><%= message.message  %> </strong>
      </p>
      <% } %>
      <div class="table-responsive">
        <table class="table table-striped text-center">
          <thead>
            <tr class="table-dark">
              <th>#</th>
              <th>Name</th>
              <th>Image</th>
              <th>Price</th>
              <th>Wholesale</th>
              <th>Deller</th>
              <th>Amount</th>
              <th>Barcode</th>
              <th>Category</th>
              <th>Action</th>
            </tr>
          </thead>
          <tbody>
            <%# for(let i = 0; i<products.length;i++){ %>
                          <td><%#= i+1 %> #</td>
            <td></td>
            <td><img src="" alt="pro" /></td>
            <td>50</td>
            <td>45</td>
            <td>30</td>
            <td>5</td>
            <td>20506070804030</td>
            <td>Accessories</td>
            <td>
              <a href="" class="text-success"
                ><i class="fas fa-edit fa-lg mx-1"></i
              ></a>
              <a href="" class="text-danger"
                ><i class="fas fa-trash fa-lg mx-1"></i
              ></a>
            </td>
              <%# } %> 
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>

<%# Footer %> <%-include('./parts/footer.ejs') %>
